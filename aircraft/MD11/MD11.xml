    <flight_control name="FCS: MD11">
        <channel name="Pitch">
            <component name="Pitch Trim Sum" type="SUMMER">
                <input>fcs/elevator-cmd-norm</input>
                <input>fcs/pitch-trim-cmd-norm</input>
                <clip>
                    <min>-1</min>
                    <max>1</max>
                </clip>
            </component>

            <component name="Elevator Control" type="AEROSURFACE_SCALE">
                <input>fcs/pitch-trim-sum</input>
                <range>
                    <min>-0.35</min>
                    <max>0.3</max>
                </range>
                <output>fcs/elevator-pos-rad</output>
            </component>
        </channel>
        <channel name="Roll">
            <component name="Roll Trim Sum" type="SUMMER">
                <input>fcs/aileron-cmd-norm</input>
                <input>fcs/roll-trim-cmd-norm</input>
                <clip>
                    <min>-1</min>
                    <max>1</max>
                </clip>
            </component>

            <component name="Left Aileron Control" type="AEROSURFACE_SCALE">
                <input>fcs/roll-trim-sum</input>
                <range>
                    <min>-0.35</min>
                    <max>0.35</max>
                </range>
                <output>fcs/left-aileron-pos-rad</output>
            </component>

            <component name="Right Aileron Control" type="AEROSURFACE_SCALE">
                <input>-fcs/roll-trim-sum</input>
                <range>
                    <min>-0.35</min>
                    <max>0.35</max>
                </range>
                <output>fcs/right-aileron-pos-rad</output>
            </component>
        </channel>
        <channel name="Yaw">
            <component name="Rudder Command Sum" type="SUMMER">
                <input>fcs/rudder-cmd-norm</input>
                <input>fcs/yaw-trim-cmd-norm</input>
                <clip>
                    <min>-1</min>
                    <max>1</max>
                </clip>
            </component>

            <component name="Yaw Damper Rate" type="SCHEDULED_GAIN">
                <input>velocities/r-aero-rad_sec</input>
                <table>
                    <independentVar>aero/qbar-psf</independentVar>
                    <tableData>
                        3.00  0.00
                       11.00  2.00
                    </tableData>
                </table>
            </component>

            <component name="Yaw Damper Beta" type="SCHEDULED_GAIN">
                <input>aero/beta-rad</input>
                <table>
                    <independentVar>aero/qbar-psf</independentVar>
                    <tableData>
                        3.00  0.00
                       11.00  0.00
                    </tableData>
                </table>
            </component>

            <component name="Yaw Damper Sum" type="SUMMER">
                <input>fcs/yaw-damper-beta</input>
                <input>fcs/yaw-damper-rate</input>
                <clip>
                    <min>-0.2</min>
                    <max>0.2</max>
                </clip>
            </component>

            <component name="Yaw Damper Final" type="SCHEDULED_GAIN">
                <input>fcs/yaw-damper-sum</input>
                <table>
                    <independentVar>aero/qbar-psf</independentVar>
                    <tableData>
                        2.99  0.00
                        3.00  1.00
                    </tableData>
                </table>
            </component>

            <component name="Rudder Sum" type="SUMMER">
                <input>fcs/rudder-command-sum</input>
                <input>fcs/yaw-damper-final</input>
                <clip>
                    <min>-1</min>
                    <max>1</max>
                </clip>
            </component>

            <component name="Rudder Control" type="AEROSURFACE_SCALE">
                <input>fcs/rudder-sum</input>
                <range>
                    <min>-0.35</min>
                    <max>0.35</max>
                </range>
                <output>fcs/rudder-pos-rad</output>
            </component>
        </channel>
        <channel name="Flaps">
            <component name="Flaps Control" type="KINEMAT">
                <input>fcs/flap-cmd-norm</input>
                <traverse>
                    <setting>
                        <position>0</position>
                        <time>0</time>
                    </setting>
                    <setting>
                        <position>15</position>
                        <time>4</time>
                    </setting>
                    <setting>
                        <position>30</position>
                        <time>3</time>
                    </setting>
                </traverse>
                <output>fcs/flap-pos-deg</output>
            </component>
        </channel>
        <channel name="Landing Gear">
            <component name="Gear Control" type="KINEMAT">
                <input>gear/gear-cmd-norm</input>
                <traverse>
                    <setting>
                        <position>0</position>
                        <time>0</time>
                    </setting>
                    <setting>
                        <position>1</position>
                        <time>5</time>
                    </setting>
                </traverse>
                <output>gear/gear-pos-norm</output>
            </component>
        </channel>
        <channel name="Speedbrake">
            <component name="Speedbrake Control" type="KINEMAT">
                <input>fcs/speedbrake-cmd-norm</input>
                <traverse>
                    <setting>
                        <position>0</position>
                        <time>0</time>
                    </setting>
                    <setting>
                        <position>1</position>
                        <time>1</time>
                    </setting>
                </traverse>
                <output>fcs/speedbrake-pos-norm</output>
            </component>
        </channel>
    </flight_control>
