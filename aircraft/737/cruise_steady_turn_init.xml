<?xml version="1.0"?>
<initialize name="cruise with steady turn">
  <!--
    This file sets up the aircraft to start off
    at cruise conditions, but in a steady turn
  -->
  <altitude unit="FT"> 25000.0 </altitude>
  <vt unit="FT/SEC">     750.0 </vt>
  <gamma unit="DEG">       0.0 </gamma>
  <latitude unit="DEG">   47.0 </latitude>
  <longitude unit="DEG"> 122.0 </longitude>
  <phi unit="DEG">        30.0 </phi>
  <psi unit="DEG">       225.0 </psi>
  <trim_config>
    <search>
      <max_iterations value="10000"/>
    </search>
    <phi/>
    <nlf step_size="0.1">   1.155 </nlf>
    <psi/>
    <gamma/>
    <throttle_cmd step_size="0.2">  0.8 </throttle_cmd>
    <elevator_cmd step_size="0.3"> -0.2 </elevator_cmd>
    <aileron_cmd step_size="0.1">   0.0 </aileron_cmd>
    <rudder_cmd step_size="0.05">    0.0 </rudder_cmd>
    <output_file name="cruise_trim_steady_turn_data.csv"/>
  </trim_config>
</initialize>
