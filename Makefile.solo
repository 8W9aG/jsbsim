clean:
	-mv *.*~ backup
	-rm *.o

all:
	touch *.cpp
	cd filtersjb; make all -fMakefile.solo; cd ..
	cd simgear/props; make all -fMakefile.solo; cd ..
	make JSBSim -fMakefile.solo

debug:
	touch *.cpp
	touch filtersjb/*.cpp
	touch simgear/props/*.cxx
	cd filtersjb; make debug CCOPTS=-g -fMakefile.solo; cd ..
	cd simgear/props; make debug CCOPTS=-g -fMakefile.solo; cd ..
	make JSBSim CCOPTS=-g -fMakefile.solo
dist:
	tar -cvf JSBSim_source.tar $(JSBSim_sources)
	gzip JSBSim_source.tar

