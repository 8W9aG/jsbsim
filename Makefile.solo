JSBSim_sources = FG*.cpp FG*.h JSBSim.cpp *.?xx filtersjb/*.cpp filtersjb/*.h simgear/props/*.?xx\
Makefile.* aircraft/*/*.xml engine/*.xml scripts/*.xml *ake* */*ake* */*/*ake*

JSBSim : $(JSBSim_objects) JSBSim.o libFCSComponents.a libProperties.a
	$(CC) $(INCLUDES) $(CCOPTS) $(LINKDIR) $(JSBSim_objects) JSBSim.o -oJSBSim -lm -lFCSComponents -lProperties

libFCSComponents.a:
	cd filtersjb; make -fMakefile.solo; cd ..

libProperties.a:
	cd simgear/props; make -fMakefile.solo; cd ../../

