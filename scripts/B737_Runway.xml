<?xml version="1.0"?>
<runscript name="B737 Runway test">
  <!--
    This run is for testing the 737 model on the runway
  -->

  <use aircraft="737"/>
  <use initialize="reset00"/>
  <run start="0.0" end="20" dt="0.0083333">
    <when>
      <parameter name="sim-time-sec" comparison="ge" value="10"/>
      <set name="fcs/steer-cmd-norm" value="0.5" action="FG_RAMP" tc="1.0"/>
    </when>
  </run>
</runscript>
