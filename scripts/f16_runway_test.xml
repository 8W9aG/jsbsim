<?xml version="1.0"?>
<runscript name="F-16 aircraft runway test">
  <!--
    This run is for testing an F-16 just sitting on the runway
  -->

  <use aircraft="f16"/>
  <use initialize="reset00"/>
  <run start="0.0" end="30" dt="0.00833333">

    <event name="15-second check">
      <condition> sim-time-sec >= 15.00 </condition>
      <notify> <property>aero/coefficient/CLDh</property> </notify>
    </event>
  
  </run>
</runscript>